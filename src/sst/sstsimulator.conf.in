[SSTCore]
MPICXX=@MPI_CXX_COMPILER@
MPICC=@MPI_C_COMPILER@
MPI_CPPFLAGS=-I@MPI_CXX_INCLUDE_DIRS@
CXX=@CMAKE_CXX_COMPILER@
CXXFLAGS=@CMAKE_CXX_FLAGS@
CC=@CMAKE_C_COMPILER@
CFLAGS=@CMAKE_C_FLAGS@
LD=@SST_LD@
LDFLAGS=@SST_LDFLAGS@
LIBS=-l@CMAKE_DL_LIBS@
CPP=@CMAKE_C_COMPILER@ -E
CXXCPP=@CMAKE_CXX_COMPILER@ -E
AR=
NM=
PYTHON=@Python_EXECUTABLE@
PYTHON_CONFIG=@PYCONFIG@
PYTHON_CPPFLAGS=@SST_PYTHON_CPPFLAGS@
PYTHON_LDFLAGS=@SST_PYTHON_LDFLAGS@
PYTHON_VERSION3=yes
LIBZ_CPPFLAGS=-I@ZLIB_INCLUDE_DIRS@
LIBZ_LDFLAGS=@ZLIB_LIBRARIES@
LIBZ_LIBS=-lz
ELEMENT_CXXFLAGS=@CMAKE_CXX_FLAGS@ -D__STDC_FORMAT_MACROS -fPIC -DHAVE_CONFIG_H -I@CMAKE_INSTALL_PREFIX@/include
ELEMENT_LDFLAGS=-shared -fno-common -Wl,-undefined -Wl,dynamic_lookup 
pkgconfig=@CMAKE_INSTALL_PREFIX@/lib/pkgconfig/SST-@VERSION@.pc
sourcedir=@CMAKE_CURRENT_SOURCE_DIR@/src/sst/core
builddir=@CMAKE_BINARY_DIR@
testsdir=@CMAKE_SOURCE_DIR@/../tests
prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=@CMAKE_INSTALL_PREFIX@
bindir=@CMAKE_INSTALL_PREFIX@/bin
sbindir=@CMAKE_INSTALL_PREFIX@/sbin
libexecdir=@CMAKE_INSTALL_PREFIX@/libexec
includedir=@CMAKE_INSTALL_PREFIX@/include
libdir=@CMAKE_INSTALL_PREFIX@/lib
CORE_TEST_ELEMENT_LIBDIR=@CMAKE_INSTALL_PREFIX@/lib/sstcore
PREVIEW_BUILD=
